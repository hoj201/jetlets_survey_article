@article{YoMa2007b,
	Abstract = {This paper develops a reduction theory for Dirac structures that includes, in a unified way, reduction of both Lagrangian and Hamiltonian systems. It includes the reduction of variational principles and in particular, the Hamilton-Pontryagin variational principle. It also includes reduction theory for implicit Lagrangian systems that could be degenerate and have constraints.

In this paper we focus on the special case in which the configuration manifold is a Lie group G. In our earlier papers we established the link between the Hamilton-Pontryagin principle and Dirac structures. We begin the paper with the reduction of this principle. The traditional view of Poisson reduction in this case is to reduce T*G with its natural Poisson structure to g * with its Lie-Poisson structure. However, the basic step of reducing Hamilton's phase space principle already shows that it is important to use g ‚{\"a}{\"\i} g * for the reduced space, rather than just g * . In this way, our construction includes both Euler-Poincar√{\copyright} as well as Lie-Poisson reduction. The geometry behind this procedure, which we call Lie-Dirac reduction starts with the standard (i.e., canonical) Dirac structure on T*G (which can be viewed either symplectically or from the Poisson viewpoint) and for each {\OE}º ‚{\`a}{\`a} g * , produces a Dirac structure on g ‚{\"a}{\"\i} g * . This geometry then simultaneously supports both Euler-Poincar√{\copyright} and Lie-Poisson reduction.

In the last part of the paper, we include nonholonomic constraints, and illustrate this construction with Suslov systems in nonholonomic mechanics, both from the Euler-Poincar√{\copyright} and Lie-Poisson viewpoints.},
	Author = {Yoshimura, H. and Marsden, J. E.},
	Date-Added = {2012-02-11 09:14:17 -0800},
	Date-Modified = {2013-10-27 15:26:46 +0000},
	Doi = {10.1016/S0034-4877(08)00004-9},
	Issn = {0034-4877},
	Journal = {Reports on Mathematical Physics},
	Keywords = {reduction by symmetry},
	Number = {3},
	Pages = {381 - 426},
	Title = {{Reduction of Dirac structures and the Hamilton-Pontryagin principle}},
	Url = {http://www.sciencedirect.com/science/article/pii/S0034487708000049},
	Volume = {60},
	Year = {2007},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0034487708000049},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/S0034-4877(08)00004-9}}

@article{YoMa2006a,
	Abstract = {This paper develops the notion of implicit Lagrangian systems and presents some of their basic properties in the context of Dirac structures. This setting includes degenerate Lagrangian systems and systems with both holonomic and nonholonomic constraints, as well as networks of Lagrangian mechanical systems. The definition of implicit Lagrangian systems with a configuration space Q makes use of Dirac structures on T ‚{\`a}{\'o} Q that are induced from a constraint distribution on Q as well as natural symplectomorphisms between the spaces T ‚{\`a}{\'o} T Q , T T ‚{\`a}{\'o} Q , and T ‚{\`a}{\'o} T ‚{\`a}{\'o} Q . Two illustrative examples are presented; the first is a nonholonomic system, namely a vertical disk rolling on a plane, and the second is an L‚{\"A}{\`\i}C circuit, a degenerate Lagrangian system with holonomic constraints.},
	Author = {Yoshimura, H. and Marsden, J. E.},
